{
   "compilerOptions": {
      "module": "commonjs",
      "target": "es6",
      "lib": [ "es6", "dom" ],
      "moduleResolution": "node",
      "outDir": "./build",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "allowJs": true,
      "noImplicitAny": true,
      "noImplicitThis": true,
      "strictNullChecks": true,
      "noImplicitReturns": true,
      "sourceMap": true
   }
}